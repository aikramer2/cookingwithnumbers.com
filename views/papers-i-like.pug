extends layout
block content
  link(rel="stylesheet" type="text/css" href="css/code.css")
  div(id="unclassified-articles") 
    h3(class="article-type") Unclassified Articles
    ul 
      if articles
        each article in articles
          li
            a(href= article.url) #{article.title}
      else
        p No articles found!   
        
